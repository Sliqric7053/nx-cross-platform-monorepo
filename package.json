{
  "name": "nx-cross-platform-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && exitzero electron-builder install-app-deps",
    "start": "nx serve",
    "build": "nx build",
    "build:mobile": "nx build --project=mobile-app-tetris",
    "open:mobile:android": "npm run build:mobile && nx run mobile-app-tetris:cap --cmd='open android'",
    "build:desktop": "nx build --project=desktop-app-tetris",
    "test": "nx test",
    "serve:web": "nx run-many --target=serve --projects=web-app-tetris,api -- --open",
    "serve:mobile": "nx run-many --target=serve --projects=mobile-app-tetris,api -- --open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.0",
